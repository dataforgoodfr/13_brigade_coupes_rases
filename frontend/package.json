{
  "name": "poc_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:cloud": "vite --mode cloud",
    "dev:mock": "vite --mode mock",
    "dev:mock:win": "set VITE_MOCK=true && vite",
    "cleanup": "biome check --write",
    "cleanup:tsc": "biome check --write && tsc",
    "build:ci": "vite build",
    "build": "pnpm cleanup && tsc -b && vite build",
    "build:preview": "pnpm build && vite preview",
    "build:dev:preview": "pnpm build --mode development && vite preview",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest --mode test-ci",
    "test:debug": "vitest --inspect --no-file-parallelism",
    "coverage": "vitest run --coverage",
    "install:playwright": "pnpm exec playwright install chromium --with-deps",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo-association-canopee.svg",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "STORYBOOK=true storybook build",
    "gen:routes": "tsr generate"
  },
